					" let result = '';",
					"    const characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';",
					"    let counter = 0;",
					"    while (counter < 7) {",
					"      result += characters.charAt(Math.floor(Math.random() * 7));",
					"      counter += 1;",
					"    }",
					"let exceedName = '';",
					"postman.setEnvironmentVariable('randomName', \"POST\"+result);",
					"    let counter1 = 0;",
					"    while (counter1 < 35) {",
					"      exceedName += characters.charAt(Math.floor(Math.random() * 7));",
					"      counter1 += 1;",
					"    }",
					"postman.setEnvironmentVariable('exceedName', \"POST\"+exceedName);",
					"",
					"utils = {",
					"    checkResponseStatus(response, status) {",
					"        //tests[\"Status code is 400\"] = response.code === status;",
					"",
					"        pm.test(\"Status code is \" + status, function () {",
					"            response.to.have.status(status);",
					"        });",
					"    },",
					"",
					"    checkError(responseBody, expectedString) {",
					"        pm.test(\"Check if the expected string exists in the response body\", function () {",
					"    pm.expect(responseBody).to.include(expectedString);",
					"    });",
					"    ",
					"    }",
					"}"
				]
			}
